friend_0 = {'first': 'kyce', 'last': 'sheppard', 'location': 'oklahoma'}
friend_1 = {'first': 'steve', 'last': 'shoesmith', 'location': 'london'}
friend_2 = {'first': 'alex', 'last': 'christmas', 'location': 'france'}

people = [friend_0, friend_1, friend_2]

for friend in people:
	print(friend)
